#include<bits/stdc++.h>
using namespace std;
int swap(long long int *a,long long int *b)
{
    *a=*a+*b;
    *b=*a-*b;
    *a=*a-*b;
    return *a;
    return *b;
}
int main()
{
    long long int n,m;
    cin>>n>>m;
    cout<<"elements to be swapped are "<<n<<"and"<<m;
    swap(&n,&m);
    cout<<n<<endl<<m;
    cout<<"element after swapping are"<<n<<"and"<<m;
    return 0;
}
